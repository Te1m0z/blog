{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "linebreak-style": true,
        "indent": [
            true,
            "tabs",
            4
        ],
        "one-line": false,
        "max-line-length": [
            true,
            200
        ],
        "no-trailing-whitespace": false
    },
    "rulesDirectory": []
}
