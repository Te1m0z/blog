{
  "name": "blog-vue-express",
  "version": "1.2.0",
  "private": "true",
  "main": "./app/server.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "ts-node-dev --watch --clear app/server.ts",
    "dev": "concurrently --kill-others \"tsc -w\" \"nodemon dist/server.js\"",
    "knex:migrate": "knex --knexfile alalal.js  migrate:latest",
    "knex:seed": "knex --knexfile alalal.js  seed:run",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "better-sqlite3": "^7.6.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "sqlite3": "^5.0.11",
    "transliteration": "^2.3.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/knex": "^0.16.1",
    "@types/node": "^18.7.15",
    "@types/sqlite3": "^3.1.8",
    "concurrently": "^7.4.0",
    "dotenv": "^16.0.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "typescript": "^4.8.2",
    "winston": "^3.8.2"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 4,
    "semi": false,
    "useTabs": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "printWidth": 90
  }
}
